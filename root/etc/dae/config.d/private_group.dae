group {
	proxy {
		filter: !name(keyword: "移动", keyword: "联通", keyword: "电信")
		policy: min_moving_avg
	}

	random {
		policy: random
	}

	tor {
		filter: name(tor)
		policy: fixed(0)
	}

	cloudflare {
		filter: name(keyword: cloudflare)
		policy: min_moving_avg
	}

	ai {
		filter: name(
		keyword: "🇬🇧", keyword: "英国",
		keyword: "🇮🇳", keyword: "印度",
		keyword: "🇯🇵", keyword: "JP", keyword: "日本",
		keyword: "🇰🇷", keyword: "KR", keyword: "韩国",
		keyword: "🇵🇭", keyword: "PH", keyword: "菲律宾",
		keyword: "🇸🇬", keyword: "SG", keyword: "新加坡",
		keyword: "🇹🇷", keyword: "TR", keyword: "土耳其",
		keyword: "🇺🇸", keyword: "US", keyword: "美国"
		) && !name(
		keyword: "移动", keyword: "联通", keyword: "电信",
		keyword: "中转", keyword: "CloudFlare"
		)
		policy: min_moving_avg
	}

	ehentai {
		filter: name(
		keyword: "🇫🇷", keyword: "FR", keyword: "法国",
		keyword: "🇺🇸", keyword: "US", keyword: "美国")
		policy: min_moving_avg
	}

	steam {
		policy: min_moving_avg
        	tcp_check_url: "http://test.steampowered.com"
	}
}
