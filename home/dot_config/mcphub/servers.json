	    ],
	    "command": "uvx",
	    "autoApprove": [
		"fetch"
	    ]
	},
	"git": {
	    "command": "uvx",
	    "args": ["mcp-server-git"],
	    "autoApprove": [
		"git_status",
		"git_diff_unstaged",
		"git_diff_staged",
		"git_diff",
		"git_log",
		"git_show",
		"git_branch"
	    ]
	    
	},
	"puppeteer": {
	    "args": [
		"-y",
		"@modelcontextprotocol/server-puppeteer"
	    ],
	    "command": "npx"
	},
	"sequential-thinking": {
	    "args": [
		"-y",
		"@modelcontextprotocol/server-sequential-thinking"
	    ],
	    "command": "npx",
	    "autoApprove": [
		"sequentialthinking"
	    ]
	},
	"time": {
	    "args": [
		"mcp-server-time",
		"--local-timezone",
		"Asia/Shanghai"
	    ],
	    "command": "uvx",
	    "autoApprove": [
		"get_current_time",
		"convert_time"
	    ]
