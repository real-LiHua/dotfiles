		"get_current_address",
		"get_current_function",
		"get_entry_points",
		"decompile_function",
		"declare_c_type",
		"check_connection",
		"get_function_by_address",
		"get_function_by_name",
		"get_metadata",
		"get_xrefs_to",
		"get_xrefs_to_field",
		"set_comment"
